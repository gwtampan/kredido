<h1>ADMIN - USERS</h1>
<section class="get">
    <h3>PRODUCTS ITEM</h3>
    <a href="/admin/user/get-data">LOAD DATA</a>
    <div>
        {{# each items }}
        <article class="item">
            <div>Email: {{ this.email }}</div>
            <div>Password: {{ this.password }}</div>
            <div>ID: {{ this._id }}</div>
        </article>
        {{/each}}
    </div>
</section>

